<div class="jumbotron"><h1>Contacts</h1></div>
<br>
<br>
<% @contacts.each do |contact| %>
<div class="panel panel-default">
  <div class="panel-body">
    <h1>Name: <small><a href= <%= "/contacts/#{contact.id.to_i}" %>> <%= contact.first_name %> <%= contact.last_name %></a></small></h1>
  </div>
  <div class="panel-footer">  
    <h3>Email: <small><%= contact.email %></small></h3>
    <h3>Phone Number: <small><%= contact.phone_number %></small></h3>   
  </div>
</div>
<% end %>


